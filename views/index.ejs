  <script>
    document.addEventListener('DOMContentLoaded', async function() {
      // Assuming moodData is passed from the server and available here
      const moodData = <%- JSON.stringify(moodData) %>;
  
      // Map mood selections to numerical values
      const moodMap = {
        "Very Unpleasant": 1,
        "Unpleasant": 2,
        "Slightly Unpleasant": 3,
        "Neutral": 4,
        "Slightly Pleasant": 5,
        "Pleasant": 6,
        "Very Pleasant": 7
      };
  
      const dates = moodData.map(entry => new Date(entry.date).toLocaleDateString());
      const moods = moodData.map(entry => moodMap[entry.mood]);
  
      // Create the chart
      const ctx = document.getElementById('moodChart').getContext('2d');
      const moodChart = new Chart(ctx, {
        type: 'line',
        data: {
          labels: dates,
          datasets: [{
            label: 'Mood',
            data: moods,
            borderColor: 'rgba(75, 192, 192, 1)',
            backgroundColor: 'rgba(75, 192, 192, 0.2)',
            fill: true,
          }]
        },
        options: {
          scales: {
            y: {
              beginAtZero: true,
              min: 1,
              max: 7,
              ticks: {
                callback: function(value, index, values) {
                  return Object.keys(moodMap).find(key => moodMap[key] === value);
                }
              }
            }
          }
        }
      });
    });
  </script>
  

